<BarHor@Label>:
	size_hint:None,None
	height:10
	font_size:dp(8)
	text: format((self.level*10),".1f") +"%"
	color: 1,1,1,1
	level: 0
	pos_hint:{"center_x":.5,"center_y":.5}

	canvas.before:
		Color:
			rgba:1,1,1,1
		Rectangle:
			size:101,self.height
			pos:self.pos
		Color:
			rgba:0,0,0,.6
		Rectangle:
			size: self.level*10,self.height-2
			pos:self.x+1, self.center_y-4


<BarVer@Label>:
	size_hint:None,None
	width:10
	height:101
	#text: format(self.level,".1f") +"%"
	font_size:dp(8)
	color:0,0,0,.5
	level:0
	pos_hint:{"center_x":.5,"center_y":.5}
	valign:"top"
	canvas.before:
		Color:
			rgba:1,1,1,1
		Rectangle:
			size: 10,self.height
			pos:self.pos
		Color:
			rgba:0,0,0,.6
		Rectangle:
			size: self.width-2,self.level
			pos:self.center_x-4, self.y+1

<StatsScreen>:
	canvas:
		Color:
			rgba: 1,0,1,.75
		Rectangle:
			size:self.width, self.height
			pos:self.pos
			#source:"Pics/Magnus.jpg"
	Image:
		source:"Pics/sjamaan1.jpg"
		size_hint:None,None
		color:1,0,1,.25
		width:800
		height:900
		allow_stretch:True
		keep_ratio:False
		pos_hint:{"center_x":.5, "center_y":.5}

	BoxLayout:
		orientation:"vertical"
		pos_hint:{"center_x":.5, "center_y":.8}
		size_hint:1,.2
		padding:20
		spacing:0
		canvas:
			Color:
				rgba: 0,0,0,0
			Rectangle:
				size:self.width, self.height
				pos:self.pos
		Label:
			text:"RunenMaster\nProgress Statistics"
			color:1,1,1,1
			font_size:dp(28)
			halign:"center"

		BoxLayout:
			size_hint:0.5,1
			pos_hint:{"center_x":.5, "center_y":.5}
			Label:
				id: _datetimes
				text:""
				color:1,1,1,1
				font_size:dp(10)
				halign:"center"
			Label:
				id: _currenttime
				text:""
				color:1,1,1,1
				font_size:dp(10)
				halign:"center"


	ScrollView:
		size_hint:.85,1
		pos_hint:{"center_x":.5, "center_y":0.15}
		do_scroll_x:False
		do_scroll_y:True
		bar_color:0.2,1,0,.45
		bar_width:15
		bar_pos_y:"right"
		scroll_type:["bars","content"]
		#scroll_y:0

		BoxLayout:
			orientation:"vertical"
			size_hint_y:None
			height:1500

			# box1
			BoxLayout:
				id: _box1
				orientation:"vertical"
				size_hint_y:None
				padding:20
				canvas:
					Color:
						rgba: 0,0,0,0
					Rectangle:
						size:self.width, self.height
						pos:self.pos
				Label:
					text:"Overall statistics"
					color:1,1,1,1
					font_size:dp(16)
					size_hint:.5,1
					halign:"left"

			# box1
			BoxLayout:
				orientation:"horizontal"
				size_hint_y:None
				height:300
				canvas:
					Color:
						rgba: 1,1,1,1
					Rectangle:
						size:self.width, self.height
						pos:self.pos
				GridLayout:
					cols:2
					spacing:20
					padding:20
					canvas:
						Color:
							rgba: 0,0,0,.1
						Rectangle:
							size:self.width, self.height
							pos:self.pos

					BoxLayout:
						orientation: "vertical"
						size_hint:.6,1
						canvas:
							Color:
								rgba: 0,0,1,.5
							Rectangle:
								size:self.width-5, self.height-5
								pos:self.pos
								source:"Pics/Runenmaster.png"

						BoxLayout:
							orientation:"vertical"
							size_hint:1,.2
							Label:
								text: "CURRENT LEVEL"
								font_size:dp(18)
								color:1,1,1,.75
								valign:"center"
								halign:"center"
						BoxLayout:
							orientation: "vertical"
							size_hint:1,.6
							Pie1:
								id:_pie1
								level: _pie2.level*.25+_pie3.level*.25+_pie4.level*.5

						BoxLayout:
							orientation:"vertical"
							size_hint:1,.2
							Label:
								id:_masterlabel
								text:"MASTER SKILLS" if _pie1.level > 324 else ("MEDIOR SKILLS" if (_pie1.level >180 and _pie1.level <324) else "APPRENTICE SKILLS")
								color:1,1,1,1
								font_size:dp(16)
								valign:"center"
								halign:"center"

